<div class="container">
<h1 class='list-title' id="header-centered"><%= markdown_to_html "#{@user.name || @user.email}'s Profile"%></h1>
  <br>
   <div class="container">
   <h4 id="list-centered"> To-Do list </h4>
    <table class="table table-striped table-hover">
      <thead>
        <tr>
          <th>Description </th>
          <th>Days Left </th>
          <th>Complete </th>
        </tr>
      </thead>
      <tbody>
        <%= render @items %> 
      <tbody>  
   </table>
   <br>
   <div>
       <%= render :template => "/items/_form.html.erb", locals: { user: @user, item: @item } %>
 </div>
</div>
</div>
